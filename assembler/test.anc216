import module
use lolly as 10
use asdrubale as 60
use pippo


structure My_struct:
    name: word,
    last_name: word,
    age: byte

section .text
org 10 + lolly * 4 + asdrubale
_code:
    var x: byte = 28    ; create a variable saved into the stack
    var y: [10 byte]
    store y[0], 500
    store y[1], 20
    store y[2], 50
    load r0, * 10
    load r1, & 10 + lolly * 4 + asdrubale
    jmp loop
exit:
    load r1, & 12
    load r4, message
    store & bp + l0, 10

loop:
    load r0, 9
    add r0, 10
    cmp r0, 200
    jls loop
    jmp exit

message:
    "ciao"